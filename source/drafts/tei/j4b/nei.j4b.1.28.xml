    <text>
        <body>
            
            <div1 type="letter">
                <pb facs="nei.j4b.1.28"/>
                <note type="curatorial">28</note>
                
                <opener>
                    <fw type="authorial">
                        Missouri Collection
                    </fw> 
                </opener>
                
                <p>Enid's birth</p>

            </div1>
            
            <div1 type="postcard">
                <pb facs="nei.j4b.1.28"/>
                <note type="curatorial"></note>
                
                <ab type="postmark">
                    NOV 4 1230 1911 <figure/>
                </ab>
                
                <ab type="stamp">
                    U.S. Post Service TWO CENTS
                </ab> 
                
                <ab type="address">
                    Mr. &amp; Mrs. John G. Neihardt, 
                    Bancroft, Nebraska.
                </ab> 
            </div1>
                
            <div1 type="postcard">
                <pb facs="nei.j4b.1.28"/>
                <note type="curatorial"></note>
                
                <fw type="postmark">
                    OMAHA NE OCT 24 1917
                    UNION DEPOT POS<figure/>
                </fw> 
                 
                <fw type="authorial">
                    Barkalow Bros Publisher Omaha Neb <figure/> 
                    POST CARD 
                    QUALITY SERIES
                </fw> 
                
                <opener>
                    <salute>Dear Maxine -</salute>
                </opener> 
                
                <p>This is the place we think of buying. The ladies are thrown in.</p>
                <closer>
                    <salute>Lovingly,</salute>
                    <signed>Jno</signed>
                </closer> 
                
                <fw type="address">
                    Mrs. Neihardt 
                    Bancroft
                    Nebraska 
                    U.S. 
                    A65603
                </fw>

            </div1>
            
        </body>
        
