    <text>
        <body>
            
            <div1 type="letter">
                
                <pb facs="nei.j4a.15.4a"/>
               
                <fw type="curatorial">4</fw>
                
                <figure>
                    <figDesc>Image of Mark Twain's face within a circle</figDesc>
                    <p>MARK TWAIN SOCIETY</p>
                </figure>
                
                <note type="unknown">Palo Alto</note>
                
                <opener>
                    <dateline>
                        Mayfield, Calif.
                        <note>Mar 28, 1930</note>
                    </dateline>
                
                    <salute>Dear Sir:</salute>
                </opener>
                <p>We are establishing a Mark Twain Library. Will you please inscribe your favorite book,</p>
                <closer>
                    <salute>Yours sincerely,</salute>
                
                    <signed>Cyril Clemens</signed>
                </closer>
                
                <note type="curatorial" place="bottom">Missouri Collection</note>
                
            </div1>
            
            
            <div1 type="enclosure">
            
                <pb facs="nei.j4a.15.4b"/>
                
                <figure/>
            
                <p>
                    90-630 <hi rend="caps">MAYFIELD-STANFORD BRANCH</hi> 90-630     No.____________
                        <lb/>
                    Bank of Italy
                        <lb/>
                    <hi rend="caps">NATIONAL TRUST &amp; SAVINGS ASSOCIATION</hi>
                        <lb/>
                    <hi rend="caps">MAYFIELD, CAL.</hi> <date><add><unclear>March</unclear> 27</add>  19<add>30</add></date>
                
                <hi rend="caps">PAY TO THE ORDER OF</hi>   <add>John <hi rend="caps">G.</hi> Neihardt</add>       $<add>2.00
                
                Two-----------------------</add> <hi rend="caps">DOLLARS</hi>
                </p>
                <figure/>
                
                <signed>Cyril Clemens</signed>
                
            </div1>
        </body>
        
