    <text>
        <body>
            
            <div1 type="letter">
                
                <pb facs="nei.j4a.15a"/>
                
                <note type="curatorial">239 Neihardt Collection</note>
                <fw type="curatorial">3</fw>
                
                <opener>
                    <dateline>New York, March 15, 1930</dateline>
                
                    <salute>My dear Neihardt:</salute>
                </opener>
                <p>Enclosed is a news item which you might care to use in the "Post-Dispatch". If you can do so, it will be deeply appreciated.</p>
                <closer>
                    <salute>With kindest regards,</salute>
                    <salute>Ever gratefully and sincerely yours,</salute>
                
                    <signed>John Myers O'Hara</signed>
                </closer>
                
            </div1>
            
            
            <div1 type="envelope">
                
                <pb facs="nei.j4a.15b"/>
                
                <ab type="return">
                    John <hi rend="caps">M.</hi> O'Hara
                    684 <unclear>Riverside Drive</unclear>,
                    <hi rend="caps">N.Y.</hi> City
                </ab>
                
                <note type="recipient"><hi rend="underline">2nd letter</hi></note>
                
                <ab type="postmark">
                    GRA<unclear reason="missing"/>NV3
                    <unclear reason="missing"/>
                    1030 PM
                    1930
                </ab>
                <figure>
                    <figDesc>2 Cent Stamp</figDesc>
                    <p>UNITED STATES POSTAGE 2 CENTS 2</p>
                </figure>
                
                <ab type="address">
                    <address>
                        <addrLine>John <hi rend="caps">G.</hi> Neihardt, Esq.</addrLine>
                            <addrLine>Branson,</addrLine>
                                <addrLine>Missouri.</addrLine>
                    </address>
                </ab>
                
            </div1>
        </body>
        
        <back></back>
    </text>
