    <text>
        <body>
            
            <div1 type="envelope">
                
                <pb facs="nei.j4a.21.24a"/>
                
                <note type="curatorial">Missouri Collection</note>
                <fw type="curatorial">24</fw>
                
                <hi rend="caps">THIS SIDE OF CARD IS FOR ADDRESS</hi>
                
                <ab type="postmark">
                    PINE RIDGE
                    JUL 5
                    12 M
                    1947
                    S. DAK.
                </ab>
                <figure>
                    <figDesc>1 Cent Stamp</figDesc>
                    <p>U.S. POSTA<unclear/> CAR<unclear/> ONE CE<unclear/> 1 JEFFERSON 1</p>
                </figure>
                
                <ab type="address">
                    <address>
                        <addrLine>Hon. John <hi rend="caps">G.</hi> Neihardt.</addrLine>
                            <addrLine>Branson</addrLine>
                                <addrLine>Mo.</addrLine>
                    </address>
                </ab>
                
            </div1>
            
            
            <div1 type="letter">
                
                <pb facs="nei.j4a.21.24b"/>
                
                <opener>
                    <dateline>
                        Pine Ridge <hi rend="caps">S. D.</hi>
                        July 5-47,
                    </dateline>
                    
                    <salute>Friend:</salute></opener> <p>My victory celebration here again is July 7-10th, Hope you can attend again,</p>
                <closer>
                    <salute>yours&#8212;</salute>
                    <signed>Henry Standing Bear</signed>
                </closer>
                
            </div1>
        </body>
        
        <back></back>
    </text>
