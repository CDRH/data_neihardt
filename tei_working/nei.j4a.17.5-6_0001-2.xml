                        <addrLine>John <hi rend="caps">G.</hi> Neihardt.</addrLine>
                            <addrLine>St. Louis,</addrLine>
                                <addrLine>Mo.</addrLine>
                
                        <addrLine>Post-Dispatch.</addrLine>
                        <addrLine>Editorial Dept.</addrLine>
                    </address>
                </ab>
                
            </div1>
